<%= link_to "Back to Event", booking_path %><br />
<% if @photo.image.resource_type == "image" %>
<%= cl_image_tag(@photo.image.url) %>
<% else %>
<%= cl_video_tag(@photo.image.url.delete_suffix(".mov"), :controls => true, :fallback_content => "Your browser does not support HTML5 video tags") %>
<% end %>
